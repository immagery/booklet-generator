                <ITEXT CPARENT="Red Italic" CH="V. "/>
                <ITEXT CPARENT="italic" CH="Lamb of God, you take away the sins of the world, "/>
                <para PARENT="centered prayer"/>
                <ITEXT CPARENT="Red Italic" CH="R. "/>
                <ITEXT CPARENT="italic" CH="Spare us, O Lord. "/>
                <para PARENT="centered prayer"/>
                <ITEXT CPARENT="Red Italic" CH="V."/>
                <ITEXT CPARENT="italic" CH=" Lamb of God, you take away the sins of the world, "/>
                <para PARENT="centered prayer"/>
                <ITEXT CPARENT="Red Italic" CH="R. "/>
                <ITEXT CPARENT="italic" CH="Graciously hear us, O Lord. "/>
