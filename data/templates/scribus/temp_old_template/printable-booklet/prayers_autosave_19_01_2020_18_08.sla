                <para PARENT="centered prayer"/>
                <ITEXT CPARENT="italic" CH="1. The Baptism of Jesus "/>
                <para PARENT="centered prayer" LINESPMode="0" LINESP="5"/>
                <ITEXT CPARENT="italic" CH="2. The Wedding Feast at Cana "/>
                <para PARENT="centered prayer" LINESPMode="0" LINESP="5"/>
                <ITEXT CPARENT="italic" CH="3. The Proclamation of the Kingdom and the call to Conversion "/>
                <para PARENT="centered prayer" LINESPMode="0" LINESP="5"/>
                <ITEXT CPARENT="italic" CH="4. The Transfiguration "/>
                <para PARENT="centered prayer" LINESPMode="0" LINESP="5"/>
