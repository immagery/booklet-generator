                <ITEXT CPARENT="italic" CH="1. The Annunciation "/>
                <para PARENT="centered prayer" LINESPMode="0" LINESP="5"/>
                <ITEXT CPARENT="italic" CH="2. The Visitation "/>
                <para PARENT="centered prayer" LINESPMode="0" LINESP="5"/>
                <ITEXT CPARENT="italic" CH="3. The Birth of Our Lord "/>
                <para PARENT="centered prayer" LINESPMode="0" LINESP="5"/>
                <ITEXT CPARENT="italic" CH="4. The Presentation "/>
                <para PARENT="centered prayer" LINESPMode="0" LINESP="5"/>
                <ITEXT CPARENT="italic" CH="5. The Finding of the Child Jesus in the Temple"/>
                <para PARENT="centered prayer" LINESPMode="0" LINESP="5"/>
