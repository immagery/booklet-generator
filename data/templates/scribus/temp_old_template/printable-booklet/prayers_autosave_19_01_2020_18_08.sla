                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Mother most pure, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Mother most chaste, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Mother inviolate,"/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Mother undefiled, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Mother most lovable, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Mother most admirable, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Mother of good counsel, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Mother of our Creator, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Mother of our Saviour, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Virgin most prudent, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Virgin most venerable, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Virgin most renowned"/>
                <ITEXT CH=","/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Virgin most powerful,"/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH=" Virgin most merciful, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Virgin most faithful, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Mirror of justice, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Seat of wisdom, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Cause of our joy, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Spiritual vessel,"/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Vessel of honour, "/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Singular vessel of devotion,"/>
                <para PARENT="Centered prayer with space"/>
                <ITEXT CPARENT="italic" CH="Mystical rose, "/>
                <para PARENT="Centered prayer with space"/>
